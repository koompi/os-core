# Description: An arbitrary precision numeric processing language
# URL: https://koompi.org        
# Maintainer:  HangsiaHONG, hangsia@koompi.org
# Organization:  KOOMPI, koompi.com
# Depends on:  

name=libisofs
version=1.5.2
release=1
source=(http://files.libburnia-project.org/releases/libisofs-1.5.2.tar.gz)

build() {
        cd $name-$version

        ./configure --prefix=/usr --disable-static && make

        make DESTDIR=$PKG install

}

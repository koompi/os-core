name=libassuan 
version=2.5.4 
release=1
source=(https://www.gnupg.org/ftp/gcrypt/libassuan/libassuan-2.5.4.tar.bz2)

build() {
        cd $name-$version

        ./configure --prefix=/usr 
        make
        make DESTDIR=$PKG install
}
